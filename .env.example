# Airbnb MCP Server Configuration

# Caching
CACHE_TTL=300  # Cache time-to-live in seconds (default: 5 minutes)

# Request Settings
REQUEST_TIMEOUT=30.0  # Request timeout in seconds
USER_AGENT=ModelContextProtocol/1.0 (Autonomous; +https://github.com/modelcontextprotocol/servers)

# Airbnb Settings
AIRBNB_BASE_URL=https://www.airbnb.com

# Robots.txt (for testing only)
IGNORE_ROBOTS_TXT=false

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR
