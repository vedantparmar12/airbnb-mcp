LIVEKIT_URL= 
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# OpenAI Configuration (Optional - only if using OpenAI TTS)
# Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Cartesia Configuration (Free tier available)
# Get from https://play.cartesia.ai/
CARTESIA_API_KEY=

# Deepgram Configuration (Required)
# Get from https://console.deepgram.com
DEEPGRAM_API_KEY=

# Model Selection (Optional)
# Choose which OpenAI model to use
LLM_CHOICE=gpt-4.1-mini

# Groq Configuration (Free - Production LLM)
# Get from https://console.groq.com
GROQ_API_KEY=

# Ollama Configuration (for local LLM - Development only)
# Download models from https://ollama.com/library
OLLAMA_MODEL=gpt-oss:20b-cloud 
OLLAMA_BASE_URL=http://localhost:11434/v1

# Development Settings (Optional)
LOG_LEVEL=INFO
DEBUG_MODE=false
